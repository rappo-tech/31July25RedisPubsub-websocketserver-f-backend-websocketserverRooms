generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model ClothCategory{
id String @id @default(uuid())
categoryName String
allCloths AllCloths[]
}
model  AllCloths{
id String @id @default(uuid())
categoryId String
category ClothCategory @relation(fields:[categoryId],references:[id],onDelete:Cascade)
clothName String
}